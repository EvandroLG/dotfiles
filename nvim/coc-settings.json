{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "prettier.onlyUseLocalVersion": true,
  "suggest.timeout": 4000,
  "eslint.filetypes": [
    "js",
    "jsx",
    "ts",
    "tsx"
  ],
  "eslint.autoFixOnSave": true,
  "tslint.autoFixOnSave": true,
  "snippets.extends": {
    "typescript": [
      "javascript"
    ],
    "javascriptreact": [
      "javascript"
    ]
  }
}
