{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "css",
    "markdown"
  ],
  "prettier.onlyUseLocalVersion": true,
  "suggest.timeout": 4000,
  "eslint.autoFixOnSave": true,
  "tslint.autoFixOnSave": true,
  "snippets.extends": {
    "typescript": [
      "javascript"
    ],
    "javascriptreact": [
      "javascript"
    ]
  }
}
