{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.formatOnSave": true,
  "coc.preferences.useQuickfixForLocations": true,
  "prettier.onlyUseLocalVersion": true,
  "suggest.timeout": 4000,
  "eslint.autoFixOnSave": true,
  "tslint.autoFixOnSave": true,
  "snippets.extends": {
    "typescript": [
      "javascript"
    ],
    "javascriptreact": [
      "javascript"
    ]
  },
  "snippets.ultisnips.pythonPrompt": false,
  "typescript.autoClosingTags": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "java.jdt.ls.vmargs": "-javaagent:/usr/local/share/lombok/lombok-1.18.30.jar",
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.jdt.ls.java.home": "/Library/Java/JavaVirtualMachines/amazon-corretto-17.jdk/Contents/Home",
  "metals.javaHome": "/Library/Java/JavaVirtualMachines/amazon-corretto-17.jdk/Contents/Home",
  "metals.enable": true
}
